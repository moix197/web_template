{
  "name": "base-template",
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "postinstall": "flowbite-react patch && flowbite-react register"
  },
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false
    }
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "turbo": "^2.4.4",
    "typescript": "^5"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.17",
    "eslint-config-next": "^15.2.4",
    "flowbite-react": "^0.11.5",
    "next": "^15.2.4",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.0.17"
  }
}